<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="0"
                android:centerColor="#3096F8"
                android:endColor="#5800C5"
                android:startColor="#3096F8"
                android:type="linear" />
        </shape>
    </item>
    <item
        android:bottom="10dp"
        android:drawable="?fabDrawable"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp" />
    <item>
        <ripple
            android:color="#fff"
            android:radius="22dp" />
    </item>
</layer-list>